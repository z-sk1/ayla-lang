{
  "name": "ayla",
  "displayName": "ayla",
  "description": "ayla lang support with syntax highlighting and ayla icon",
  "publisher": "z-sk1",
  "version": "0.1.4",
  "icon": "icon.png",
  "repository": {
    "url": "https://github.com/z-sk1/ayla-lang"
  },
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "ayla",
      "aliases": ["ayla", "ayla"],
      "extensions": [".ayl",".ayla"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "ayla",
      "scopeName": "source.ayla",
      "path": "./syntaxes/ayla.tmLanguage.json"
    }],
    "iconThemes": [
      {
        "id": "ayla-icons",
        "label": "ayla Icons",
        "path": "./icons/ayla-file-icons.json"
      }
    ]
  }
}

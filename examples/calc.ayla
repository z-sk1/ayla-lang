fun add(x float, y float) {
    back x + y;
}

fun multiply(x, y) {
    back x * y;
}

fun subtract(x, y) {
    back x - y;
}

fun divide(x, y) {
    back x / y;
}

egg firstNum;
egg oper;
egg secondNum;

explodeln("Firstnum");
scanln(firstNum);

explodeln("Operator");
scanln(oper);
 
explodeln("Secondnum");
scanln(secondNum);

ayla oper == "+" {
    explodeln(firstNum + oper + secondNum + "=" + toString(add(toFloat(firstNum), toFloat(secondNum))));
} elen ayla oper == "-" {
    explodeln(firstNum + oper + secondNum + "=" + toString(subtract(toFloat(firstNum), toFloat(secondNum))));
} elen ayla oper == "*" {
    explodeln(firstNum + oper + secondNum + "=" + toString(multiply(toFloat(firstNum), toFloat(secondNum))));
} elen ayla oper == "/" {
    explodeln(firstNum + oper + secondNum + "=" + toString(divide(toFloat(firstNum), toFloat(secondNum))));
}